   <property name="styleSheet">
    <string notr="true">/* Стили для контейнера select */
.custom-select {
    position: relative;
    display: inline-block;
    width: 200px; /* Ширина контейнера */
}

/* Скрываем оригинальный select */
.custom-select select {
    display: none; 
}

/* Стили для отображаемого элемента */
.select-selected {
    background-color: #f1f1f1;
    padding: 10px;
    border: 1px solid #ccc;
    border-radius: 5px;
    cursor: pointer;
}

/* Стили для выпадающего списка */
.select-items {
    position: absolute;
    background-color: #f9f9f9;
    border: 1px solid #ccc;
    border-radius: 5px;
    z-index: 99;
    display: none; /* Скрыт по умолчанию */
    max-height: 150px; /* Максимальная высота */
    overflow-y: auto; /* Прокрутка при переполнении */
}

/* Стили для элементов выпадающего списка */
.select-items div {
    padding: 10px;
    cursor: pointer;
}

/* Эффект при наведении на элементы */
.select-items div:hover {
    background-color: #ddd;
}

/* Стрелка для выпадающего списка */
.select-arrow {
    position: absolute;
    right: 10px;
    top: 14px;
    border: solid transparent;
    border-width: 5px 5px 0 5px;
    border-top-color: #333; /* Цвет стрелки */
}</string>
